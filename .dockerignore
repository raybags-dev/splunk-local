# Ignore Docker-related files and directories
.dockerignore
Dockerfile*
docker-compose*

# Ignore build output
build/
dist/
*.log

# Ignore node modules (if using Node.js)
node_modules/

# Ignore Python cache (if using Python)
__pycache__/
*.pyc

# Ignore temporary files
*.swp
*.tmp
*.bak
*~

# Ignore version control
.git
.gitignore

# Ignore environment files
.env
.env.*

# Ignore OS generated files
.DS_Store
Thumbs.db